.container {
    display: flex;
    width: 100%;
    height: 320rpx;
    justify-content: center;
    align-items: center;
    background-color: #fff;
}

.inner-container {
    display: flex;
    width: 730rpx;
    height: 300rpx;
    border: 1px dashed #DCEBE6;
}

.grid-item {
    width: 200rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.img {
    width: 60rpx;
    height: 60rpx;
}

.text {
    margin-top: 6px;
    font-size: 24rpx;
    color: #333;
}